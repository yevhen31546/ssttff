  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "qa": "ng build --env=qa --output-hashing=all --aot=true --build-optimizer=true",
    "build": "ng build --prod --output-hashing=all --aot=true --build-optimizer=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:frontend": "ng run stf-frontend:server && webpack --config webpack.server.config.js --progress --colors",
